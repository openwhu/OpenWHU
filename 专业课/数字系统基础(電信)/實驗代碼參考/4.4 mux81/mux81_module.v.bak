module mux81_module
(
	CSn, Sw_In, Key_In0, Key_In1, Key_In2, Key_In3, Key_In4, Key_In5,Key_In6, Key_In7, LED_Out
);
	 input CSn;
	 input [2:0]Sw_In;
	 input [width-1:0]Key_In0, Key_In1, Key_In2, Key_In3, Key_In4, Key_In5,Key_In6, Key_In7;	 
	 output reg [width-1:0]LED_Out;

	 parameter width;//因按键开关个数限制，八选一数据选择器的输入输出的位宽设计为1

	always @ ( * )
		begin 
			if(!CSn)			//CSn为低电平使多路选择器工作
				case(Sw_In)
					3'b000: LED_Out <= Key_In0;
					3'b001: LED_Out <= Key_In1;
					3'b010: LED_Out <= Key_In2;
					3'b011: LED_Out <= Key_In3;
					3'b100: LED_Out <= Key_In4;
					3'b101: LED_Out <= Key_In5;
					3'b110: LED_Out <= Key_In6;
					3'b111: LED_Out <= Key_In7;
				endcase
			else				//CSn为高电平时，多路选择器不工作
				LED_Out <= 0;
		end
	
	assign addr = Sw_In 

endmodule 