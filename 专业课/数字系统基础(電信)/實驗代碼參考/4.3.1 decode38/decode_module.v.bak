module decode_module
(
	Sw_In, LED_Out
);
	input wire [2:0]Sw_In;
	output wire [7:0]LED_Out;

	assign LED_Out[0] = ~Sw_In[2] & ~Sw_In[1] & ~Sw_In[0];//While Sw_In=3'b000ï¼ŒLED_Out=8'b00000001
	assign LED_Out[1] = ~Sw_In[2] & ~Sw_In[1] & Sw_In[0];
	assign LED_Out[2] = ~Sw_In[2] & Sw_In[1] & ~Sw_In[0];
	assign LED_Out[3] = ~Sw_In[2] & Sw_In[1] & Sw_In[0];
	assign LED_Out[4] = Sw_In[2] & ~Sw_In[1] & ~Sw_In[0];
	assign LED_Out[5] = Sw_In[2] & ~Sw_In[1] & Sw_In[0];
	assign LED_Out[6] = Sw_In[2] & Sw_In[1] & ~Sw_In[0];
	assign LED_Out[7] = Sw_In[2] & Sw_In[1] & Sw_In[0];

endmodule
